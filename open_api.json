{
  "openapi": "3.0.3",
  "info": {
    "title": "Django API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8000"
    }
  ],
  "paths": {
    "/api/product/{id}/": {
      "get": {
        "operationId": "retrieveProduct",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "string"
            },
            "required": true
          }
        ],
        "tags": [
          "productAPI"
        ],
        "security": [
          {
            "cookieAuth": []
          },
          {
            "tokenAuth": []
          },
          {}
        ],
        "responses": {
          "200": {
            "description": "Successful retrieval of product details",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Product"
                }
              }
            }
          }
        }
      },
      "post": {
        "operationId": "createNewProduct",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "string"
            },
            "required": true
          }
        ],
        "tags": [
          "productAPI"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Product"
              }
            },
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/Product"
              }
            },
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/Product"
              }
            }
          }
        },
        "security": [
          {
            "cookieAuth": []
          },
          {
            "tokenAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Product created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Product"
                }
              }
            }
          }
        }
      },
      "put": {
        "operationId": "updateExistingProduct",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "string"
            },
            "required": true
          }
        ],
        "tags": [
          "productAPI"
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Product"
              }
            },
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/Product"
              }
            },
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/Product"
              }
            }
          }
        },
        "security": [
          {
            "cookieAuth": []
          },
          {
            "tokenAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Product updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Product"
                }
              }
            }
          }
        }
      },
      "delete": {
        "operationId": "deleteProduct",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "string"
            },
            "required": true
          }
        ],
        "tags": [
          "productAPI"
        ],
        "security": [
          {
            "cookieAuth": []
          },
          {
            "tokenAuth": []
          }
        ],
        "responses": {
          "204": {
            "description": "Product deleted successfully (no response body)"
          }
        }
      }
    },
    "/datatb/product/": {
      "get": {
        "operationId": "listProducts",
        "tags": [
          "dataTable"
        ],
        "summary": "List products in the data table",
        "parameters": [
          {
            "in": "query",
            "name": "entries",
            "schema": {
              "type": "string",
              "pattern": "^[0-9]+/?$"
            },
            "required": false,
            "description": "Number of entries to return, e.g., '100' or '100/'"
          }
        ],
        "responses": {
          "200": {
            "description": "A list of products",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Product"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "operationId": "createProduct",
        "tags": [
          "dataTable"
        ],
        "summary": "Create a new product in the data table",
        "requestBody": {
          "required": true,
          "description": "Product object to be created",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Product"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Product created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Product"
                }
              }
            }
          }
        }
      },
      "put": {
        "operationId": "updateAllProducts",
        "tags": [
          "dataTable"
        ],
        "summary": "Update all products in the data table",
        "requestBody": {
          "required": true,
          "description": "Product object with updated data for all products",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Product"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Products updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Product"
                }
              }
            }
          }
        }
      },
      "delete": {
        "operationId": "deleteAllProducts",
        "tags": [
          "dataTable"
        ],
        "summary": "Delete all products in the data table",
        "responses": {
          "204": {
            "description": "All products deleted successfully (no response body)"
          }
        }
      }
    },
    "/datatb/product/export/": {
      "get": {
        "operationId": "exportProductToPDF",
        "tags": [
          "dataTable"
        ],
        "summary": "Export products to PDF",
        "parameters": [
          {
            "in": "query",
            "name": "format",
            "schema": {
              "type": "string"
            },
            "required": false,
            "description": "Format of the exported file, e.g., 'pdf'"
          }
        ],
        "responses": {
          "200": {
            "description": "PDF export successful",
            "content": {
              "application/pdf": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        }
      },
      "post": {
        "operationId": "generateExport",
        "tags": [
          "dataTable"
        ],
        "summary": "Generate a new export of products",
        "requestBody": {
          "required": true,
          "description": "Export configuration settings",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "search": {
                    "type": "string",
                    "example": ""
                  },
                  "hidden_cols": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "example": []
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "pdf"
                    ],
                    "example": "pdf"
                  }
                },
                "required": [
                  "search",
                  "hidden_cols",
                  "type"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Export generated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "url": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "operationId": "updateExportConfig",
        "tags": [
          "dataTable"
        ],
        "summary": "Update export configuration",
        "requestBody": {
          "required": true,
          "description": "New export configuration",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "format": {
                    "type": "string",
                    "example": "pdf"
                  },
                  "template": {
                    "type": "string",
                    "example": "default"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Export configuration updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "delete": {
        "operationId": "deleteExportHistory",
        "tags": [
          "dataTable"
        ],
        "summary": "Delete export history",
        "responses": {
          "204": {
            "description": "Export history deleted successfully (no response body)"
          }
        }
      }
    },
    "/datatb/product/edit/{id}/": {
      "get": {
        "operationId": "retrieveProductForEdit",
        "tags": [
          "dataTable"
        ],
        "summary": "Retrieve product data for editing",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "description": "Product ID to edit (e.g., 0 may trigger a crash)"
          }
        ],
        "responses": {
          "200": {
            "description": "Product data retrieved for editing",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Product"
                }
              }
            }
          }
        }
      },
      "post": {
        "operationId": "updateProductInTable",
        "tags": [
          "dataTable"
        ],
        "summary": "Update an existing product in the data table",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "description": "Product ID to update (allowing 0 for crash triggering)"
          }
        ],
        "requestBody": {
          "required": true,
          "description": "Product object with updated data",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Product"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Product updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Product"
                }
              }
            }
          }
        }
      },
      "put": {
        "operationId": "replaceProductInTable",
        "tags": [
          "dataTable"
        ],
        "summary": "Replace an existing product in the data table",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "description": "Product ID to replace"
          }
        ],
        "requestBody": {
          "required": true,
          "description": "New product object data",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Product"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Product replaced successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Product"
                }
              }
            }
          }
        }
      },
      "delete": {
        "operationId": "deleteProductFromTable",
        "tags": [
          "dataTable"
        ],
        "summary": "Delete a product from the data table",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "description": "Product ID to delete"
          }
        ],
        "responses": {
          "204": {
            "description": "Product deleted successfully (no response body)"
          }
        }
      }
    },
    "/datatb/product/delete/{id}/": {
      "get": {
        "operationId": "retrieveProductForDeletion",
        "tags": [
          "dataTable"
        ],
        "summary": "Retrieve product data before deletion",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "description": "Product ID to delete"
          }
        ],
        "responses": {
          "200": {
            "description": "Product data retrieved for deletion confirmation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Product"
                }
              }
            }
          }
        }
      },
      "post": {
        "operationId": "confirmProductDeletion",
        "tags": [
          "dataTable"
        ],
        "summary": "Confirm deletion of a product",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "description": "Product ID to confirm deletion"
          }
        ],
        "requestBody": {
          "required": true,
          "description": "Confirmation data",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "confirm": {
                    "type": "boolean",
                    "example": true
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Product deletion confirmed",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "Product deleted successfully"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "put": {
        "operationId": "updateDeletionSettings",
        "tags": [
          "dataTable"
        ],
        "summary": "Update deletion settings for a product",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "description": "Product ID for deletion settings"
          }
        ],
        "requestBody": {
          "required": true,
          "description": "Deletion settings",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cascade": {
                    "type": "boolean",
                    "example": true
                  },
                  "permanent": {
                    "type": "boolean",
                    "example": false
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Deletion settings updated",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "updated"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "delete": {
        "operationId": "executeProductDeletion",
        "tags": [
          "dataTable"
        ],
        "summary": "Permanently delete a product",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "schema": {
              "type": "integer"
            },
            "required": true,
            "description": "Product ID to permanently delete"
          }
        ],
        "responses": {
          "204": {
            "description": "Product permanently deleted (no response body)"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AuthToken": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "writeOnly": true,
            "example": [
              "alice",
              "bob",
              "charlie"
            ]
          },
          "password": {
            "type": "string",
            "writeOnly": true,
            "example": [
              "secret",
              "admin123",
              "password1"
            ]
          },
          "token": {
            "type": "string",
            "readOnly": true,
            "example": [
              "abc123",
              "def456",
              "ghi789"
            ]
          }
        },
        "required": [
          "username",
          "password",
          "token"
        ]
      },
      "Product": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "readOnly": true,
            "example": [
              100
            ],
            "maximum": 9223372036854776000,
            "minimum": -9223372036854776000
          },
          "name": {
            "type": "string",
            "maxLength": 2,
            "example": [
              "pd"
            ]
          },
          "info": {
            "type": "string",
            "maxLength": 2,
            "example": [
              "dt"
            ]
          },
          "price": {
            "type": "integer",
            "maximum": 9223372036854776000,
            "minimum": -9223372036854776000,
            "format": "int64",
            "nullable": true,
            "example": [
              99,
              -99
            ]
          }
        },
        "required": [
          "id",
          "name"
        ]
      }
    },
    "securitySchemes": {
      "cookieAuth": {
        "type": "apiKey",
        "in": "cookie",
        "name": "sessionid"
      },
      "tokenAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "Authorization",
        "description": "Token based authentication. Include the prefix \"Token\" followed by your key."
      }
    }
  }
}
